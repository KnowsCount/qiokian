{
	"name": "qiokian",
	"version": "2.2.14",
	"description": "ðŸ™Š live2d anime figure vuejs component.",
	"main": "dist/qiokian.ssr.js",
	"browser": "dist/qiokian.esm.js",
	"module": "dist/qiokian.esm.js",
	"unpkg": "dist/qiokian.min.js",
	"files": [
		"dist/*",
		"src/**/*.vue"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KnowsCount/qiokian.git"
	},
	"keywords": [
		"vuejs",
		"library",
		"component",
		"vue",
		"vuejs2",
		"component-library",
		"lib",
		"rollup",
		"live2d",
		"i18n"
	],
	"author": "KnowsCount <knowscount@gmail.com>",
	"license": "GPLv3",
	"sideEffects": false,
	"scripts": {
		"serve": "vue-cli-service serve dev/serve.js",
		"build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
		"build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
		"build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
		"build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
		"prepublish": "npm run build",
		"prepare": "husky install",
		"test": "jest --collectCoverage"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@rollup/plugin-alias": "^3.1.1",
		"@rollup/plugin-babel": "^5.2.2",
		"@rollup/plugin-commonjs": "^20.0.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"@rollup/plugin-replace": "^3.0.0",
		"@vue/cli-plugin-babel": "^4.5.10",
		"@vue/cli-service": "^3.12.1",
		"@vue/test-utils": "^1.2.1",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^27.0.2",
		"cross-env": "^7.0.3",
		"eslint": "^7.28.0",
		"eslint-plugin-vue": "^8.4.1",
		"husky": "^7.0.0",
		"jest": "^27.0.4",
		"minimist": "^1.2.5",
		"rollup": "^2.36.1",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-vue": "^5.1.9",
		"vue": "^2.6.12",
		"vue-jest": "^3.0.7",
		"vue-template-compiler": "^2.6.12"
	},
	"peerDependencies": {
		"vue": "^2.6.12"
	},
	"engines": {
		"node": ">=12"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm --no-git-tag-version version patch && git add ."
		}
	},
	"jest": {
		"moduleNameMapper": {
			"^vue$": "vue/dist/vue.common.js"
		},
		"moduleFileExtensions": [
			"js",
			"vue"
		],
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
		},
		"testEnvironment": "jsdom"
	}
}
